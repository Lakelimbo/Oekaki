{
	"name": "oekaki.web",
	"version": "0.0.1",
	"type": "module",
	"author": {
		"name": "Lakelimbo",
		"email": "lakelimbo@proton.me",
		"url": "https://lakelimbo.github.io"
	},
	"scripts": {
		"start": "npm run typedocs && npm run dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"typedocs": "npx openapi-typescript $services__coreapi__http__0/swagger/v1/swagger.json -o ./src/lib/data/openapi-schema-v1.d.ts"
	},
	"devDependencies": {
		"@internationalized/date": "^3.6.0",
		"@playwright/test": "^1.45.3",
